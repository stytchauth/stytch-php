{
    "name": "stytch/stytch-php",
    "description": "Stytch PHP SDK",
    "keywords": ["stytch", "authentication", "sdk"],
    "homepage": "https://stytch.com",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.86"
    },
    "autoload": {
        "psr-4": {
            "Stytch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stytch\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Stytch",
            "email": "support@stytch.com"
        }
    ]
}
